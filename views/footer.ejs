  <script src="/socket.io/socket.io.js"></script>
<script>


  var socket = io.connect(location.href);  

  

    socket.on('statusr', function (data) {
        console.log(data);
        document.getElementById("checkr").innerHTML = data.r;

    });

    socket.on('statusnew', function (data) {
        console.log("status new",data);
        let status = data.val;
        let myBW = document.getElementById("2111");

        let bg, fg = '';

        // myBW.querySelectorAll('.bedNo');

        switch(status){
          case "0" :
            bg = "red";
            fg = "white";
            break;
          case "1" :
            bg = "yellow";
            fg = "green";
            break;
          case "2" :
            bg = "green";
            fg = "yellow";
            break;
        }

        myBW.style.backgroundColor = bg ;
        myBW.style.color = fg ;

    });

    socket.on('newconnect', function (data) {
        console.log(data);
        // document.getElementById("checkr").innerHTML = "new Connect " + data.ip;
    });

    
</script>

<script>

  function oncheckstatus(s){
        socket.emit(`checkstatus`, { s });
  }


    
</script>


